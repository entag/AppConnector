.row
	.col.s12.card
		.card-content
			span.card-title Solutions
			.row
				.input-field.col.s12
					select#solutionsSoftware
						option(value='Box') Box
						option(value='DocuSign') DocuSign
						option(value='Shoeboxed') Shoeboxed
					label(for='solutionsSoftware') Sofware
			.row
				.input-field.col.s8
					select#solutionsLicence
						option(value='Sole Trader') Sole Trader
						option(value='small business') Small business
					label(for='solutionsLicence') Licence
				.input-field.col.s4
					select#solutionsQty
						option(value='1') 1
						option(value='2') 2
						option(value='3') 3
						option(value='4') 4
						option(value='5') 5
						option(value='6') 6
						option(value='7') 7
						option(value='8') 8
						option(value='9') 9
						option(value='10') 10
					label(for='solutionsQty') Qty
			.row
				.input-field.col.s12
					select#supportpackage
						option(value='None') None
						option(value='Support 5 hours') Support 5 Hours
						option(value='Support 10 hours') Support 10 Hours
					label(for='supportpackage') Support Package
