		.navbar-header
			a.navbar-brand(href='#') Entag Partner Portal
		ul.nav.navbar-nav
			if user
				li
					a(href='#') New Order
		ul.nav.navbar-nav.navbar-right
			unless user
				li
					a(href='#')
						span.glyphicon.glyphicon-user
						span Sign Up
				li
					a(href='#')
						span.glyphicon.glyphicon-log-in
